  q[1]:=((x1-x[0])*(x1-x[2])*(x1-x[3])*(x1-x[4])*(x1-x[5])*(x1-x[6]))/((x[1]-x[0])*(x[1]-x[2])*(x[1]-x[3])*(x[1]-x[4])*(x[1]-x[5])*(x[1]-x[6]));
  q[2]:=((x1-x[0])*(x1-x[1])*(x1-x[3])*(x1-x[4])*(x1-x[5])*(x1-x[6]))/((x[2]-x[0])*(x[2]-x[1])*(x[2]-x[3])*(x[2]-x[4])*(x[2]-x[5])*(x[2]-x[6]));
  q[3]:=((x1-x[0])*(x1-x[1])*(x1-x[2])*(x1-x[4])*(x1-x[5])*(x1-x[6]))/((x[3]-x[1])*(x[3]-x[2])*(x[3]-x[0])*(x[3]-x[4])*(x[3]-x[5])*(x[3]-x[6]));
  q[4]:=((x1-x[1])*(x1-x[2])*(x1-x[3])*(x1-x[0])*(x1-x[5])*(x1-x[6]))/((x[4]-x[1])*(x[4]-x[2])*(x[4]-x[3])*(x[4]-x[0])*(x[4]-x[5])*(x[4]-x[6]));
  q[5]:=((x1-x[1])*(x1-x[2])*(x1-x[3])*(x1-x[4])*(x1-x[0])*(x1-x[6]))/((x[5]-x[1])*(x[5]-x[2])*(x[5]-x[3])*(x[5]-x[4])*(x[5]-x[0])*(x[5]-x[6]));
  q[6]:=((x1-x[1])*(x1-x[2])*(x1-x[3])*(x1-x[4])*(x1-x[5])*(x1-x[0]))/((x[6]-x[1])*(x[6]-x[2])*(x[6]-x[3])*(x[6]-x[4])*(x[6]-x[5])*(x[6]-x[0]));
  for var i := 0 to 7 do begin
    writeln ('  q',i,' = ',q[i]:3:2);
  end;
  L6[7]:=F[0]*q[0]+F[1]*q[1]+F[2]*q[2]+F[3]*q[3]+F[4]*q[4]+F[5]*q[5]+F[6]*q[6];
  writeln ('L6[7] = ',L6[7]:5:4);
  //погрешность интерполяции
  min:= L7-L6[0];
  for var i:= 1 to 7 do begin
    if abs(L7-L6[i]) < min then min:= L7-L6[i];
  end;
  writeln(min);
end.